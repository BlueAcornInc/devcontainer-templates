{
    "id": "adobe-commerce-and-magento",
    "version": "0.0.1",
    "name": "Adobe Commerce and Magento",
    "description": "Run and manage Adobe Commerce and Magento Open Source projects",
    "documentationURL": "https://github.com/doughatcher/devcontainer-templates/tree/main/src/commerce",
    "publisher": "doug@hatcher.ltd",
    "licenseURL": "https://github.com/devcontainers/templates/blob/main/LICENSE",
    "options": {
        "phpVersion": {
            "type": "string",
            "description": "The version of PHP to install, should align with Commerce version",
            "default": "8.3",
            "proposls": ["7.4", "8.1", "8.2", "8.3", "8.4"]
        },
        "composerVersion": {
            "type": "string",
            "description": "The version of Composer to install",
            "default": "2.7.0",
            "proposals": ["2.6.0", "2.7.0", "2.8.0"]
        }
    },
    "platforms": ["Any"],
    "optionalPaths": [
        ".github/*"
    ]
}